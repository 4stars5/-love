#include<stdio.h>
#include<string.h>
int main()
{
	char str[1000];
	int i=0;
	int ans;
	gets(str);
	for(i=1;i<strlen(str);i++)
	{
		if(str[i]=='*')
	    {
		    str[i+1]=(str[i-1]-'0')*(str[i+1]-'0')+'0';
		    str[i-1]='1';
		    str[i]='*'; 
		}
	    else if(str[i]=='/')
		{
		    str[i+1]=(str[i-1]-'0')/(str[i+1]-'0')+'0';
		    str[i]='*';
		    str[i-1]='1';
		}
	}
	
	for(i=1;i<strlen(str);i++)
	{
		if(str[i]=='*')
		{
		    str[i+1]=(str[i-1]-'0')*(str[i+1]-'0')+'0';
		    str[i]=str[i-2];
		    str[i-1]='0';
		}
	}
	
	ans=str[0]-'0';
	
	for(i=1;i<strlen(str);i++)
	{
			if(str[i]=='+')
			{
			    ans=ans+(str[i+1]-'0');
		    }
		    else if(str[i]=='-')
		    {
			    ans=ans-(str[i+1]-'0');
		    }
	} 
	printf("ans=%d\n",ans);
	return 0;
}
